{
  "name": "Untappd-2-Hoptimaal",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Search your selected text Hoptimaal.nl",
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus"
  ],
  "commands": {
    "my-command": {
      "description": "Search your selected text Hoptimaal.nl",
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      }
    }
  },
    "icons": {
    "128": "favicon.ico"
  },
  "background": {
    "service_worker": "background.js"
  }
}
